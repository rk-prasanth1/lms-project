pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sh 'cd webapp && npm install'
                sh 'cd webapp && npm run build'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
